

<form method="post" action="/owners/signup">
    <h3>Your Info:</h3>

    <p><strong>Contact Info:</strong><br>
    <p><label for="fname">First Name:</label><input type="text" name="fname">
    <label for="lname">Last Name:</label><input type="text" name="lname"></p>
    <label for="phone">Phone Number:</label><input type="text" name="phone" size=14>
    </p>
    <br>
    <p><strong>Mailing Info:</strong><br>
    <label for="street">Street Address:</label><input type="text" name="street" size=40><br>

    <p><label for="city">City:</label><input type="text" name="city">
    <label for="zip">Zip Code:</label><input type="text" name="zip" size=5></p>
    </p>
    
    <br><br>
    <p><strong>Account Info:</strong></p>
    <p><label for="username">Username: <input type="text" name="username"></p>
    <p><label for="password">Password: <input type="password" name="password"></p>
    <br>
    <input type="submit" value="Create Account">
</form>