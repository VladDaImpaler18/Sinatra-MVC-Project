<p>This is the Pictures index page</p>
<% @pictures.each do |picture| %>

<h3><%= picture.title %></h3>
<figure>
<img src="<%= picture.get_picture %>" alt="<%= picture.filename %>" height="300" width="231"/><br>
<figcaption>
<%= picture.caption %>
</figcaption>
<%= picture.description %><br>
</figure>
<% end %>
